{
  "name": "example-symbol-library",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf out-tsc dist node_modules",
    "cleanout": "rimraf out-tsc dist",
    "prebuild": "npm run cleanout",
    "build": "node -e require('./build').start()",
    "predeploy": "npm run build",
    "deploy": "node deploy.js",
    "predeploy:min": "npm run build",
    "deploy:min": "node deploy.js --r",
    "prebuild-test": "npm run cleanout",
    "build-test": "tsc -p src/tsconfig.spec.json",
    "postbuild-test": "node test.js",
    "build-test:watch": "tsc -p src/tsconfig.spec.json -w",
    "certgen": "node ssl/certgen.js",
    "certinstall": "node ssl/certinstall.js",
    "certuninstall": "node ssl/certuninstall.js",
    "pretest": "npm run build-test",
    "test": "concurrently \"npm run build-test:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build-test",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "docs": "typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/ext.types.ts",
    "sync": "node package-update.js",
    "start": "nodemon server.js --watch src -e *"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "5.2.0",
    "@angular/compiler": "5.2.0",
    "@angular/core": "5.2.0",
    "@angular/forms": "5.2.0",
    "@angular/platform-browser": "5.2.0",
    "@angular/platform-browser-dynamic": "5.2.0",
    "@angular/router": "5.2.0",
    "core-js": "2.5.3",
    "express": "4.16.2",
    "rxjs": "5.5.6",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/compiler-cli": "5.2.0",
    "@types/compression": "0.0.34",
    "@types/core-js": "0.9.45",
    "@types/express": "^4.0.37",
    "@types/jasmine": "2.8.4",
    "@types/node": "^8.0.31",
    "camelcase": "^4.0.0",
    "chokidar": "^1.7.0",
    "compression": "1.7.1",
    "concurrently": "3.2.0",
    "fs-extra": "4.0.3",
    "glob": "7.1.2",
    "jasmine-core": "2.8.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.12.1",
    "rollup": "^0.41.5",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^1.0.1",
    "semver": "5.5.0",
    "systemjs": "^0.19.40",
    "ts-loader": "^0.9.5",
    "tslint": "5.9.1",
    "typedoc": "0.9.0",
    "typescript": "2.5.3",
    "@osisoft/piwebapi": "2.1.0-rc.1",
    "tslib": "^1.9.0"   
  }
}
